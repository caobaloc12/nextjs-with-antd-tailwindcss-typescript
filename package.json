{
  "name": "nextjs-with-typescript-tailwindcss-antd",
  "version": "1.0.0",
  "description": "A Next.js starter with Typescript, Ant Design and TailwindCSS.",
  "author": "Loc Cao <loccb90@gmail.com> (https://github.com/caobaloc12)",
  "private": true,
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start -p 4000",
    "type-check": "tsc",
    "lint": "eslint --fix .",
    "format": "prettier --write './**/*.{js,jsx,ts,tsx,css,md,json}' --config ./.prettierrc",
    "analyze": "cross-env ANALYZE=true next build"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "./**/*.{js,jsx,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@ant-design/icons": "4.0.0",
    "@next/bundle-analyzer": "^10.0.7",
    "@types/qs": "^6.9.5",
    "antd": "^4.12.3",
    "axios": "^0.21.1",
    "cross-env": "^7.0.3",
    "next": "^10.0.7",
    "postcss-import": "^14.0.2",
    "qs": "^6.9.6",
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
  "devDependencies": {
    "@types/node": "^12.12.21",
    "@types/react": "^16.9.16",
    "@types/react-dom": "^16.9.4",
    "@typescript-eslint/eslint-plugin": "^4.15.2",
    "@typescript-eslint/parser": "^4.15.2",
    "autoprefixer": "^10.2.6",
    "babel-plugin-import": "^1.13.3",
    "eslint": "^7.20.0",
    "eslint-config-prettier": "^8.0.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^5.1.1",
    "lint-staged": "^10.5.4",
    "postcss": "^8.3.0",
    "prettier": "^2.2.1",
    "tailwindcss": "^2.1.2",
    "typescript": "4.0"
  },
  "license": "MIT"
}